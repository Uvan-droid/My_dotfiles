    "suggest.completionItemKindLabels": {
    	"method": "  ",
    	"function": "  ",
   	"variable": "[]",
   	"field": "  ",
   	"typeParameter": "<>",
   	"constant": "  ",
   	"class": " פּ ",
   	"interface": " 蘒",
  	 "struct": "  ",
  	 "event": "  ",
  	 "operator": "  ",
   	"module": "  ",
   	"property": "  ",
   	"enum": " 練",
